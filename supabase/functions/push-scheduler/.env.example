# Copy this to .env and fill values before deploying
# Then run:
# supabase functions secrets set --project-ref <your-ref> --env-file supabase/functions/push-scheduler/.env

# Supabase
SUPABASE_URL=https://<your-project-ref>.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
SUPABASE_ANON_KEY=your_anon_key_optional

# APNs
APNS_KEY_ID=YOUR_KEY_ID
APNS_TEAM_ID=YOUR_TEAM_ID
# Paste full PEM content on a single line or keep newlines; CLI supports env-file multiline
APNS_PRIVATE_KEY="""
-----BEGIN PRIVATE KEY-----
<your-private-key>
-----END PRIVATE KEY-----
"""

# iOS bundle (hard-coded in function but you can override if needed)
BUNDLE_ID=com.chenzhencong.HiCalendar
